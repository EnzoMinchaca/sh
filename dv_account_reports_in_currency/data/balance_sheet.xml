<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="balance_usd" model="account.report">
        <field name="name">Balance USD</field>
        <field name="filter_date_range" eval="False"/>
        <field name="filter_analytic_groupby" eval="True"/>
        <field name="filter_unfold_all" eval="True"/>
        <field name="filter_journals" eval="True"/>
        <field name="filter_multi_company">selector</field>
        <field name="default_opening_date_filter">today</field>
        <field name="column_ids">
            <record id="balance_sheet_balance_copy1" model="account.report.column">
                <field name="name">Balance</field>
                <field name="expression_label">balance</field>
            </record>
            <record id="balance_sheet_balance_usd" model="account.report.column">
                <field name="name">Balance USD</field>
                <field name="expression_label">balance_usd</field>
            </record>
        </field>
        <field name="line_ids">
            <record id="account_financial_report_total_assets0_copy1" model="account.report.line">
                <field name="name">ACTIVOS</field>
                <field name="hierarchy_level">0</field>
                <field name="code">TA_USD1</field>
                <field name="expression_ids">
                    <record id="account_financial_report_TA" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">CA_USD1.balance + FA_USD1.balance + PNCA_USD1.balance</field>
                    </record>
                    <record id="account_financial_report_TA_usd" model="account.report.expression">
                        <field name="label">balance_usd</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">CA_USD1.balance_usd + FA_USD1.balance_usd + PNCA_USD1.balance_usd</field>
                    </record>
                </field>
                <field name="children_ids">
                    <record id="account_financial_report_current_assets_view0_copy1" model="account.report.line">
                        <field name="name">Activos corrientes</field>
                        <field name="code">CA_USD1</field>
                        <field name="expression_ids">
                            <record id="account_financial_report_CA" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">aggregation</field>
                                <field name="formula">BA_USD1.balance + REC_USD1.balance + CAS_USD1.balance + PRE_USD1.balance</field>
                            </record>
                            <record id="account_financial_report_CA_usd" model="account.report.expression">
                                <field name="label">balance_usd</field>
                                <field name="engine">aggregation</field>
                                <field name="formula">BA_USD1.balance_usd + REC_USD1.balance_usd + CAS_USD1.balance_usd + PRE_USD1.balance_usd</field>
                            </record>
                        </field>
                        <field name="children_ids">
                            <record id="account_financial_report_bank_view0_copy1" model="account.report.line">
                                <field name="name">Cuentas bancarias y en efectivo</field>
                                <field name="code">BA_USD1</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="expression_ids">
                                    <record id="account_financial_report_BA" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">domain</field>
                                        <field name="formula">[('account_id.account_type', '=', 'asset_cash')]</field>
                                        <field name="subformula">sum</field>
                                    </record>
                                    <record id="account_financial_report_BA_usd" model="account.report.expression">
                                        <field name="label">balance_usd</field>
                                        <field name="engine">domain</field>
                                        <field name="formula">[('account_id.account_type', '=', 'asset_cash')]</field>
                                        <field name="subformula">sum_usd</field>
                                    </record>
                                </field>
                            </record>
                            <record id="account_financial_report_receivable0_copy1" model="account.report.line">
                                <field name="name">Por cobrar</field>
                                <field name="code">REC_USD1</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="expression_ids">
                                    <record id="account_financial_report_REC" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">domain</field>
                                        <field name="formula">[('account_id.account_type', '=', 'asset_receivable'), ('account_id.non_trade', '=', False)]</field>
                                        <field name="subformula">sum</field>
                                    </record>
                                    <record id="account_financial_report_REC_usd" model="account.report.expression">
                                        <field name="label">balance_usd</field>
                                        <field name="engine">domain</field>
                                        <field name="formula">[('account_id.account_type', '=', 'asset_receivable'), ('account_id.non_trade', '=', False)]</field>
                                        <field name="subformula">sum_usd</field>
                                    </record>
                                </field>
                            </record>
                            <record id="account_financial_report_current_assets0_copy1" model="account.report.line">
                                <field name="name">Activos corrientes</field>
                                <field name="code">CAS_USD1</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="expression_ids">
                                    <record id="account_financial_report_CAS" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">domain</field>
                                        <field name="formula">['|', ('account_id.account_type', '=', 'asset_current'), '&amp;', ('account_id.account_type', '=', 'asset_receivable'), ('account_id.non_trade', '=', True)]</field>
                                        <field name="subformula">sum</field>
                                    </record>
                                    <record id="account_financial_report_CAS_usd" model="account.report.expression">
                                        <field name="label">balance_usd</field>
                                        <field name="engine">domain</field>
                                        <field name="formula">['|', ('account_id.account_type', '=', 'asset_current'), '&amp;', ('account_id.account_type', '=', 'asset_receivable'), ('account_id.non_trade', '=', True)]</field>
                                        <field name="subformula">sum_usd</field>
                                    </record>
                                </field>
                            </record>
                            <record id="account_financial_report_prepayements0_copy1" model="account.report.line">
                                <field name="name">Pre-pagos</field>
                                <field name="code">PRE_USD1</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="expression_ids">
                                    <record id="account_financial_report_PRE" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">domain</field>
                                        <field name="formula">[('account_id.account_type', '=', 'asset_prepayments')]</field>
                                        <field name="subformula">sum</field>
                                    </record>
                                    <record id="account_financial_report_PRE_usd" model="account.report.expression">
                                        <field name="label">balance_usd</field>
                                        <field name="engine">domain</field>
                                        <field name="formula">[('account_id.account_type', '=', 'asset_prepayments')]</field>
                                        <field name="subformula">sum_usd</field>
                                    </record>
                                </field>
                            </record>
                        </field>
                    </record>
                    <record id="account_financial_report_fixed_assets_view0_copy1" model="account.report.line">
                        <field name="name">Activos fijos extra</field>
                        <field name="code">FA_USD1</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="expression_ids">
                            <record id="account_financial_report_FA" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">domain</field>
                                <field name="formula">[('account_id.account_type', '=', 'asset_fixed')]</field>
                                <field name="subformula">sum</field>
                            </record>
                            <record id="account_financial_report_FA_usd" model="account.report.expression">
                                <field name="label">balance_usd</field>
                                <field name="engine">domain</field>
                                <field name="formula">[('account_id.account_type', '=', 'asset_fixed')]</field>
                                <field name="subformula">sum_usd</field>
                            </record>
                        </field>
                    </record>
                    <record id="account_financial_report_non_current_assets_view0_copy1" model="account.report.line">
                        <field name="name">Activos no corrientes extras</field>
                        <field name="code">PNCA_USD1</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="expression_ids">
                            <record id="account_financial_report_PNCA" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">domain</field>
                                <field name="formula">[('account_id.account_type', '=', 'asset_non_current')]</field>
                                <field name="subformula">sum</field>
                            </record>
                            <record id="account_financial_report_PNCA_usd" model="account.report.expression">
                                <field name="label">balance_usd</field>
                                <field name="engine">domain</field>
                                <field name="formula">[('account_id.account_type', '=', 'asset_non_current')]</field>
                                <field name="subformula">sum_usd</field>
                            </record>
                        </field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_liabilities_view0_copy1" model="account.report.line">
                <field name="name">PASIVOS</field>
                <field name="hierarchy_level">0</field>
                <field name="code">L_USD1</field>
                <field name="expression_ids">
                    <record id="account_financial_report_liabilities_view0_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">CL_USD1.balance + NL_USD1.balance</field>
                        <field name="green_on_positive" eval="False"/>
                    </record>
                    <record id="account_financial_report_liabilities_view0_balance_usd" model="account.report.expression">
                        <field name="label">balance_usd</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">CL_USD1.balance_usd + NL_USD1.balance_usd</field>
                        <field name="green_on_positive" eval="False"/>
                    </record>
                </field>
                <field name="children_ids">
                    <record id="account_financial_report_current_liabilities0_copy1" model="account.report.line">
                        <field name="name">Pasivos corrientes</field>
                        <field name="code">CL_USD1</field>
                        <field name="expression_ids">
                            <record id="account_financial_report_current_liabilities0_balance" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">aggregation</field>
                                <field name="formula">CL1_USD1.balance + CL2_USD1.balance</field>
                                <field name="green_on_positive" eval="False"/>
                            </record>
                            <record id="account_financial_report_current_liabilities0_balance_usd" model="account.report.expression">
                                <field name="label">balance_usd</field>
                                <field name="engine">aggregation</field>
                                <field name="formula">CL1_USD1.balance_usd + CL2_USD1.balance_usd</field>
                                <field name="green_on_positive" eval="False"/>
                            </record>
                        </field>
                        <field name="children_ids">
                            <record id="account_financial_report_current_liabilities1_copy1" model="account.report.line">
                                <field name="name">Pasivos corrientes</field>
                                <field name="code">CL1_USD1</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="expression_ids">
                                    <record id="account_financial_report_current_liabilities1_balance" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="['|', ('account_id.account_type', 'in', ('liability_current', 'liability_credit_card')), '&amp;', ('account_id.account_type', '=', 'liability_payable'), ('account_id.non_trade', '=', True)]"/>
                                        <field name="subformula">-sum</field>
                                        <field name="green_on_positive" eval="False"/>
                                    </record>
                                    <record id="account_financial_report_current_liabilities1_balance_usd" model="account.report.expression">
                                        <field name="label">balance_usd</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="['|', ('account_id.account_type', 'in', ('liability_current', 'liability_credit_card')), '&amp;', ('account_id.account_type', '=', 'liability_payable'), ('account_id.non_trade', '=', True)]"/>
                                        <field name="subformula">-sum_usd</field>
                                        <field name="green_on_positive" eval="False"/>
                                    </record>
                                </field>
                            </record>
                            <record id="account_financial_report_current_liabilities_payable_copy1" model="account.report.line">
                                <field name="name">Por pagar</field>
                                <field name="code">CL2_USD1</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="expression_ids">
                                    <record id="account_financial_report_current_liabilities_payable_balance" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="[('account_id.account_type', '=', 'liability_payable'), ('account_id.non_trade', '=', False)]"/>
                                        <field name="subformula">-sum</field>
                                        <field name="green_on_positive" eval="False"/>
                                    </record>
                                    <record id="account_financial_report_current_liabilities_payable_balance_usd" model="account.report.expression">
                                        <field name="label">balance_usd</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="[('account_id.account_type', '=', 'liability_payable'), ('account_id.non_trade', '=', False)]"/>
                                        <field name="subformula">-sum_usd</field>
                                        <field name="green_on_positive" eval="False"/>
                                    </record>
                                </field>
                            </record>
                        </field>
                    </record>
                    <record id="account_financial_report_non_current_liabilities0_copy1" model="account.report.line">
                        <field name="name">Pasivos adicionales no corrientes</field>
                        <field name="code">NL_USD1</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="expression_ids">
                            <record id="account_financial_report_non_current_liabilities0_balance" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">domain</field>
                                <field name="formula" eval="[('account_id.account_type', '=', 'liability_non_current')]"/>
                                <field name="subformula">-sum</field>
                                <field name="green_on_positive" eval="False"/>
                            </record>
                            <record id="account_financial_report_non_current_liabilities0_balance_usd" model="account.report.expression">
                                <field name="label">balance_usd</field>
                                <field name="engine">domain</field>
                                <field name="formula" eval="[('account_id.account_type', '=', 'liability_non_current')]"/>
                                <field name="subformula">-sum_usd</field>
                                <field name="green_on_positive" eval="False"/>
                            </record>
                        </field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_equity0_copy1" model="account.report.line">
                <field name="name">CAPITAL</field>
                <field name="hierarchy_level">0</field>
                <field name="code">EQ_USD1</field>
                <field name="expression_ids">
                    <record id="account_financial_report_EQ" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">UNAFFECTED_EARNINGS_USD1.balance + RETAINED_EARNINGS_USD1.balance</field>
                    </record>
                    <record id="account_financial_report_EQ_usd" model="account.report.expression">
                        <field name="label">balance_usd</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">UNAFFECTED_EARNINGS_USD1.balance_usd + RETAINED_EARNINGS_USD1.balance_usd</field>
                    </record>
                </field>
                <field name="children_ids">
                    <record id="account_financial_unaffected_earnings0_copy1" model="account.report.line">
                        <field name="name">Ganancias sin asignar</field>
                        <field name="code">UNAFFECTED_EARNINGS_USD1</field>
                        <field name="expression_ids">
                            <record id="account_financial_report_UE" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">aggregation</field>
                                <field name="formula">CURR_YEAR_EARNINGS_USD1.balance + PREV_YEAR_EARNINGS_USD1.balance</field>
                            </record>
                            <record id="account_financial_report_UE_usd" model="account.report.expression">
                                <field name="label">balance_usd</field>
                                <field name="engine">aggregation</field>
                                <field name="formula">CURR_YEAR_EARNINGS_USD1.balance_usd + PREV_YEAR_EARNINGS_USD1.balance_usd</field>
                            </record>
                        </field>
                        <field name="children_ids">
                            <record id="account_financial_current_year_earnings0_copy1" model="account.report.line">
                                <field name="name">Ganancias no asignadas del año en curso</field>
                                <field name="code">CURR_YEAR_EARNINGS_USD1</field>
                                <field name="expression_ids">
                                    <record id="account_financial_report_CYE" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">aggregation</field>
                                        <field name="formula">CURR_YEAR_EARNINGS_PNL_USD1.balance + CURR_YEAR_EARNINGS_ALLOC_USD1.balance</field>
                                    </record>
                                    <record id="account_financial_report_CYE_usd" model="account.report.expression">
                                        <field name="label">balance_usd</field>
                                        <field name="engine">aggregation</field>
                                        <field name="formula">CURR_YEAR_EARNINGS_PNL_USD1.balance_usd + CURR_YEAR_EARNINGS_ALLOC_USD1.balance_usd</field>
                                    </record>
                                </field>
                                <field name="children_ids">
                                    <record id="account_financial_current_year_earnings_line_1_copy1" model="account.report.line">
                                        <field name="name">Ganancias del año actual</field>
                                        <field name="code">CURR_YEAR_EARNINGS_PNL_USD1</field>
                                        <field name="expression_ids">
                                            <record id="account_financial_current_year_earnings_line_1_balance" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">aggregation</field>
                                                <field name="formula">NEP_USD1.balance</field>
                                                <field name="date_scope">from_fiscalyear</field>
                                                <field name="subformula">cross_report</field>
                                            </record>
                                            <record id="account_financial_current_year_earnings_line_1_balance_usd" model="account.report.expression">
                                                <field name="label">balance_usd</field>
                                                <field name="engine">aggregation</field>
                                                <field name="formula">NEP_USD1.balance_usd</field>
                                                <field name="date_scope">from_fiscalyear</field>
                                                <field name="subformula">cross_report</field>
                                            </record>
                                        </field>
                                    </record>
                                    <record id="account_financial_current_year_earnings_line_2_copy1" model="account.report.line">
                                        <field name="name">Ganancias asignadas el presente año</field>
                                        <field name="code">CURR_YEAR_EARNINGS_ALLOC_USD1</field>
                                        <field name="expression_ids">
                                            <record id="account_financial_current_year_earnings_line_2_balance" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">domain</field>
                                                <field name="formula" eval="[('account_id.account_type', '=', 'equity_unaffected')]"/>
                                                <field name="date_scope">from_fiscalyear</field>
                                                <field name="subformula">-sum</field>
                                            </record>
                                            <record id="account_financial_current_year_earnings_line_2_balance_usd" model="account.report.expression">
                                                <field name="label">balance_usd</field>
                                                <field name="engine">domain</field>
                                                <field name="formula" eval="[('account_id.account_type', '=', 'equity_unaffected')]"/>
                                                <field name="date_scope">from_fiscalyear</field>
                                                <field name="subformula">-sum_usd</field>
                                            </record>
                                        </field>
                                    </record>
                                </field>
                            </record>
                            <record id="account_financial_previous_year_earnings0_copy1" model="account.report.line">
                                <field name="name">Ganancias no asignados de años anteriores</field>
                                <field name="code">PREV_YEAR_EARNINGS_USD1</field>
                                <field name="expression_ids">
                                    <record id="account_financial_previous_year_earnings0_allocated_earnings" model="account.report.expression">
                                        <field name="label">allocated_earnings</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="[('account_id.account_type', '=', 'equity_unaffected')]"/>
                                        <field name="subformula">-sum</field>
                                        <field name="date_scope">from_beginning</field>
                                    </record>
                                    <record id="account_financial_previous_year_earnings0_balance_domain" model="account.report.expression">
                                        <field name="label">balance_domain</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="[('account_id.account_type', 'in', ('income', 'income_other', 'expense_direct_cost', 'expense', 'expense_depreciation'))]"/>
                                        <field name="subformula">-sum</field>
                                        <field name="date_scope">from_beginning</field>
                                    </record>
                                    <record id="account_financial_previous_year_earnings0_balance" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">aggregation</field>
                                        <field name="formula">PREV_YEAR_EARNINGS_USD1.balance_domain + PREV_YEAR_EARNINGS_USD1.allocated_earnings - CURR_YEAR_EARNINGS_USD1.balance</field>
                                    </record>
                                    <record id="account_financial_previous_year_earnings0_allocated_earnings_usd" model="account.report.expression">
                                        <field name="label">allocated_earnings_usd</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="[('account_id.account_type', '=', 'equity_unaffected')]"/>
                                        <field name="subformula">-sum_usd</field>
                                        <field name="date_scope">from_beginning</field>
                                    </record>
                                    <record id="account_financial_previous_year_earnings0_balance_domain_usd" model="account.report.expression">
                                        <field name="label">balance_domain_usd</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="[('account_id.account_type', 'in', ('income', 'income_other', 'expense_direct_cost', 'expense', 'expense_depreciation'))]"/>
                                        <field name="subformula">-sum_usd</field>
                                        <field name="date_scope">from_beginning</field>
                                    </record>
                                    <record id="account_financial_previous_year_earnings0_balance_usd" model="account.report.expression">
                                        <field name="label">balance_usd</field>
                                        <field name="engine">aggregation</field>
                                        <field name="formula">PREV_YEAR_EARNINGS_USD1.balance_domain_usd + PREV_YEAR_EARNINGS_USD1.allocated_earnings_usd - CURR_YEAR_EARNINGS_USD1.balance_usd</field>
                                    </record>
                                </field>
                            </record>
                        </field>
                    </record>
                    <record id="account_financial_retained_earnings0_copy1" model="account.report.line">
                        <field name="name">Ganancias acumuladas</field>
                        <field name="code">RETAINED_EARNINGS_USD1</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="expression_ids">
                            <record id="account_financial_report_RETAINED" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">domain</field>
                                <field name="formula">[('account_id.account_type', '=', 'equity')]</field>
                                <field name="subformula">-sum</field>
                            </record>
