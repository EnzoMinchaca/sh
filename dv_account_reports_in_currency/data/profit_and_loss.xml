<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="profit_and_loss_usd" model="account.report">
        <field name="name">Ganancias y p√©rdidas en USD</field>
        <field name="filter_analytic_groupby" eval="True"/>
        <field name="filter_unfold_all" eval="True"/>
        <field name="filter_journals" eval="True"/>
        <field name="filter_multi_company">selector</field>
        <field name="default_opening_date_filter">this_year</field>
        <field name="column_ids">
            <record id="profit_and_loss_column" model="account.report.column">
                <field name="name">Balance</field>
                <field name="expression_label">balance</field>
            </record>
            <record id="profit_and_loss_column_usd" model="account.report.column">
                <field name="name">Balance USD</field>
                <field name="expression_label">balance_usd</field>
            </record>
        </field>
        <field name="line_ids">
            <record id="account_financial_report_net_profit0_copy1" model="account.report.line">
                <field name="name">Net Profit</field>
                <field name="hierarchy_level">0</field>
                <field name="code">NEP_USD1</field>
                <field name="expression_ids">
                    <record id="account_financial_report_net_profit0_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">OPINC_USD1.balance + OIN_USD1.balance - COS_USD1.balance - EXP_USD1.balance - DEP_USD1.balance</field>
                        <field name="date_scope">normal</field>
                    </record>
                    <record id="account_financial_report_net_profit0_balance_usd" model="account.report.expression">
                        <field name="label">balance_usd</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">OPINC_USD1.balance_usd + OIN_USD1.balance_usd - COS_USD1.balance_usd - EXP_USD1.balance_usd - DEP_USD1.balance_usd</field>
                        <field name="date_scope">normal</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_totalincome0_copy1" model="account.report.line">
                <field name="name">Income</field>
                <field name="hierarchy_level">0</field>
                <field name="code">INC_USD1</field>
                <field name="expression_ids">
                    <record id="account_financial_report_totalincome0_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">OPINC_USD1.balance + OIN_USD1.balance</field>
                        <field name="date_scope">normal</field>
                    </record>
                    <record id="account_financial_report_totalincome0_balance_usd" model="account.report.expression">
                        <field name="label">balance_usd</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">OPINC_USD1.balance_usd + OIN_USD1.balance_usd</field>
                        <field name="date_scope">normal</field>
                    </record>
                </field>
                <field name="children_ids">
                    <record id="account_financial_report_gross_profit0_copy1" model="account.report.line">
                        <field name="name">Gross Profit</field>
                        <field name="code">GRP_USD1</field>
                        <field name="expression_ids">
                            <record id="account_financial_report_gross_profit0_balance" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">aggregation</field>
                                <field name="formula">OPINC_USD1.balance - COS_USD1.balance</field>
                                <field name="date_scope">normal</field>
                            </record>
                            <record id="account_financial_report_gross_profit0_balance_usd" model="account.report.expression">
                                <field name="label">balance_usd</field>
                                <field name="engine">aggregation</field>
                                <field name="formula">OPINC_USD1.balance_usd - COS_USD1.balance_usd</field>
                                <field name="date_scope">normal</field>
                            </record>
                        </field>
                        <field name="children_ids">
                            <record id="account_financial_report_income0_copy1" model="account.report.line">
                                <field name="name">Operating Income</field>
                                <field name="code">OPINC_USD1</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="expression_ids">
                                    <record id="account_financial_report_income0_balance" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="[('account_id.account_type', '=', 'income')]"/>
                                        <field name="date_scope">normal</field>
                                        <field name="subformula">-sum</field>
                                    </record>
                                    <record id="account_financial_report_income0_balance_usd" model="account.report.expression">
                                        <field name="label">balance_usd</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="[('account_id.account_type', '=', 'income')]"/>
                                        <field name="date_scope">normal</field>
                                        <field name="subformula">-sum_usd</field>
                                    </record>
                                </field>
                            </record>
                            <record id="account_financial_report_cost_sales0_copy1" model="account.report.line">
                                <field name="name">Cost of Revenue</field>
                                <field name="code">COS_USD1</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="expression_ids">
                                    <record id="account_financial_report_cost_sales0_balance" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="[('account_id.account_type', '=', 'expense_direct_cost')]"/>
                                        <field name="date_scope">normal</field>
                                        <field name="subformula">sum</field>
                                        <field name="green_on_positive" eval="False"/>
                                    </record>
                                    <record id="account_financial_report_cost_sales0_balance_usd" model="account.report.expression">
                                        <field name="label">balance_usd</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="[('account_id.account_type', '=', 'expense_direct_cost')]"/>
                                        <field name="date_scope">normal</field>
                                        <field name="subformula">sum_usd</field>
                                        <field name="green_on_positive" eval="False"/>
                                    </record>
                                </field>
                            </record>
                        </field>
                    </record>
                    <record id="account_financial_report_other_income0_copy1" model="account.report.line">
                        <field name="name">Other Income</field>
                        <field name="code">OIN_USD1</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="expression_ids">
                            <record id="account_financial_report_other_income0_balance" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">domain</field>
                                <field name="formula" eval="[('account_id.account_type', '=', 'income_other')]"/>
                                <field name="date_scope">normal</field>
                                <field name="subformula">-sum</field>
                            </record>
                            <record id="account_financial_report_other_income0_balance_usd" model="account.report.expression">
                                <field name="label">balance_usd</field>
                                <field name="engine">domain</field>
                                <field name="formula" eval="[('account_id.account_type', '=', 'income_other')]"/>
                                <field name="date_scope">normal</field>
                                <field name="subformula">-sum_usd</field>
                            </record>
                        </field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_less_expenses0_copy1" model="account.report.line">
                <field name="name">Expenses</field>
                <field name="hierarchy_level">0</field>
                <field name="code">LEX_USD1</field>
                <field name="expression_ids">
                    <record id="account_financial_report_less_expenses0_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">EXP_USD1.balance + DEP_USD1.balance</field>
                        <field name="date_scope">normal</field>
                        <field name="green_on_positive" eval="False"/>
                    </record>
                    <record id="account_financial_report_less_expenses0_balance_usd" model="account.report.expression">
                        <field name="label">balance_usd</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">EXP_USD1.balance_usd + DEP_USD1.balance_usd</field>
                        <field name="date_scope">normal</field>
                        <field name="green_on_positive" eval="False"/>
                    </record>
                </field>
                <field name="children_ids">
                    <record id="account_financial_report_expense0_copy1" model="account.report.line">
                        <field name="name">Expenses</field>
                        <field name="code">EXP_USD1</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="expression_ids">
                            <record id="account_financial_report_expense0_balance" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">domain</field>
                                <field name="formula" eval="[('account_id.account_type', '=', 'expense')]"/>
                                <field name="date_scope">normal</field>
                                <field name="subformula">sum</field>
                                <field name="green_on_positive" eval="False"/>
                            </record>
                            <record id="account_financial_report_expense0_balance_usd" model="account.report.expression">
                                <field name="label">balance_usd</field>
                                <field name="engine">domain</field>
                                <field name="formula" eval="[('account_id.account_type', '=', 'expense')]"/>
                                <field name="date_scope">normal</field>
                                <field name="subformula">sum_usd</field>
                                <field name="green_on_positive" eval="False"/>
                            </record>
                        </field>
                    </record>
                    <record id="account_financial_report_depreciation0_copy1" model="account.report.line">
                        <field name="name">Depreciation</field>
                        <field name="code">DEP_USD1</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="expression_ids">
                            <record id="account_financial_report_depreciation0_balance" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">domain</field>
                                <field name="formula" eval="[('account_id.account_type', '=', 'expense_depreciation')]"/>
                                <field name="date_scope">normal</field>
                                <field name="subformula">sum</field>
                                <field name="green_on_positive" eval="False"/>
                            </record>
                            <record id="account_financial_report_depreciation0_balance_usd" model="account.report.expression">
                                <field name="label">balance_usd</field>
                                <field name="engine">domain</field>
                                <field name="formula" eval="[('account_id.account_type', '=', 'expense_depreciation')]"/>
                                <field name="date_scope">normal</field>
                                <field name="subformula">sum_usd</field>
                                <field name="green_on_positive" eval="False"/>
                            </record>
                        </field>
                    </record>
                </field>
            </record>
        </field>
    </record>
</odoo>